<h1>Edit Pet Information</h1>

<%= form_for(@pet) do |f|%>
<label>Name:</label>
<%= f.text_field(:name) %>
<label>Type:</label>
<%= f.text_field(:pet_category) %>
<label>Breed:</label>
<%= f.text_field(:breed) %>
<label>Age:</label>
<%= f.number_field(:age) %>
<label>Gender:</label>
<%= f.text_field(:gender) %>
<label>Nickname:</label>
<%= f.text_field(:nick_name) %>
<label>Color:</label>
<%= f.text_field(:color) %>
<%= f.submit "Submit Edit" %>
<%end%>
